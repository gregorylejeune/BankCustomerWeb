            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.address_line_1)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.address_line_1)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.city)
            </dt>
            <dd class="col-sm-10">
